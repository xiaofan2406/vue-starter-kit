{
  "name": "vue-starter-kit",
  "version": "0.0.0",
  "description": "Vue starter kit",
  "scripts": {
    "dev": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --config ./config/webpack.dev.js",
    "prebuild": "rm -r -f ./build",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --config ./config/webpack.prod.js",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.js ./src",
    "preversion": "npm run lint",
    "postversion": "git push"
  },
  "engines": {
    "node": "^6.8.0",
    "npm": "^3.10.8"
  },
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
    "babel",
    "webpack",
    "eslint",
    "airbnb"
  ],
  "author": "Xiaofan Wu",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaofan2406/vue-starter-kit"
  },
  "license": "ISC",
  "dependencies": {
    "vue": "^2.0.3",
    "vue-router": "^2.0.1",
    "vuex": "^2.0.0",
    "vuex-router-sync": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "chalk": "^1.1.3",
    "css-loader": "^0.25.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-import-resolver-webpack": "^0.6.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-html": "^1.5.3",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "filesize": "^3.3.0",
    "gzip-size": "^3.0.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "node-uuid": "^1.4.7",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.13.0",
    "recursive-readdir": "^2.1.0",
    "rimraf": "^2.5.4",
    "strip-ansi": "^3.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-loader": "^9.5.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
