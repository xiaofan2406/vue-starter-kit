{
  "name": "vue-starter-kit",
  "version": "0.0.0",
  "description": "Vue starter kit",
  "scripts": {
    "dev": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --config ./config/webpack.dev.js",
    "prebuild": "rm -r -f ./build",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --config ./config/webpack.prod.js",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.js ./src",
    "preversion": "npm run lint",
    "postversion": "git push && git push --tags"
  },
  "engines": {
    "node": "^6.x",
    "npm": "^3.x"
  },
  "keywords": [
    "vue",
    "vue-router",
    "babel",
    "webpack",
    "eslint"
  ],
  "author": "Xiaofan Wu",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaofan2406/vue-starter-kit"
  },
  "license": "ISC",
  "dependencies": {
    "vue": "^2.0.5",
    "vue-router": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.10.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-import-resolver-webpack": "^0.7.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^1.6.0",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "json-loader": "^0.5.4",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.2.0",
    "postcss-loader": "^1.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-loader": "^9.9.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
